algoritmo "Exercício 7 da lista"

var
salariomin,salarioatual,final:real

inicio
//entrada
Escreva("informe o salário mínimo atualmente: ")
Leia(salariomin)
Escreva("Informe o seu salário atual: ")
Leia(salarioatual)
//processo
final:= salarioatual/salariomin
//saída
Escreva("Você recebe", final, " salários mínimos")

fimalgoritmo
